  },
  "dependencies": {
    "@nextstack/database": "workspace:*",
    "better-auth": "^1.3.9"
  },
  "devDependencies": {
    "@nextstack/tsconfig": "workspace:*",
    "@nextstack/eslint-config": "workspace:*",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2"
  }
