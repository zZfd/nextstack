{
  "name": "@nextstack/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --max-warnings 0 --fix"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.133.0",
    "@tamagui/config": "^1.133.0",
    "@tamagui/font-inter": "^1.133.0",
    "@tamagui/shorthands": "^1.133.0",
    "@tamagui/themes": "^1.133.0",
    "@tamagui/toast": "^1.133.0",
    "tamagui": "^1.133.0"
  },
  "devDependencies": {
    "@nextstack/eslint-config": "workspace:*",
    "@nextstack/tsconfig": "workspace:*",
    "@tamagui/build": "^1.133.0",
    "@types/react": "^19.0.0",
    "typescript": "^5.9.2"
  }
}